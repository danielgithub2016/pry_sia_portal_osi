<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<application>
		<resource-bundle>
			<base-name>erp_alfresco.messages</base-name>
			<var>msgAL</var>
		</resource-bundle>
	</application>

	<!-- Modulo de Alfresco -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>documentos_alfresco_listado</from-outcome>
			<to-view-id>/alfresco/paginas/documentos_alfresco_listado.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>documentos_alfresco_mantenimiento</from-outcome>
			<to-view-id>/alfresco/paginas/documentos_alfresco_mantenimiento.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>documentos_alfresco_vistaprevia</from-outcome>
			<to-view-id>/alfresco/paginas/documentos_alfresco_vistaprevia.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>documentos_alfresco_error</from-outcome>
			<to-view-id>/alfresco/paginas/documentos_alfresco_error.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
