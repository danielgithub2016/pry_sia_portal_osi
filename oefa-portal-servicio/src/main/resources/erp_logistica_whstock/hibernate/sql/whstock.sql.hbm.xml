	<!-- whstock.dw_wh_requisition_base_qt -->
	<sql-query name="whstock.DwWhRequisitionBaseQt">
		SELECT 
			WH_Cotizacion.FechaDocumento as "fechadocumento",   
			WH_Cotizacion.MonedaCodigo as "monedacodigo",   
			WH_Cotizacion.PrecioUnitario as "preciounitario"  
		FROM 
			SGCORESYS.WH_RequisicionCotizacion 
			INNER JOIN SGCORESYS.WH_RequisicionDetalle
				ON (WH_RequisicionCotizacion.CompaniaSocio = WH_RequisicionDetalle.CompaniaSocio AND
					 WH_RequisicionCotizacion.RequisicionNumero = WH_RequisicionDetalle.RequisicionNumero)
			INNER JOIN SGCORESYS.WH_Cotizacion
			ON (WH_RequisicionCotizacion.CompaniaSocio = WH_Cotizacion.CompaniaSocio AND
				 WH_RequisicionCotizacion.CotizacionSecuencia = WH_Cotizacion.CotizacionSecuencia)
		WHERE 
			( WH_RequisicionCotizacion.Secuencia = WH_RequisicionDetalle.Secuencia ) and  
			(WH_Cotizacion.CompaniaSocio = :p_compania ) AND  
			WH_Cotizacion.PrecioUnitario > 0   
	</sql-query>

