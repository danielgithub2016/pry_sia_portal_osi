	<!-- whstock.dw_wh_requisition_list -->
	<sql-query name="whstock.DwWhRequisitionList">
		 <![CDATA[
		SELECT
		rownum as "secuencia",
		WH_Requisiciones.CompaniaSocio as "companiasocio",
		WH_Requisiciones.RequisicionNumero as "requisicionnumero",
		WH_Requisiciones.FechaPreparacion as "fechapreparacion",
		WH_Requisiciones.Clasificacion as "clasificacion",
		WH_Requisiciones.Estado as "estado",
		0 AS "oc",
		WH_Requisiciones.MonedaCodigo as "monedacodigo",
		WH_Requisiciones.PrecioTotal as "preciototal",
		WH_Requisiciones.CuantiaMonetariaPendienteFlag as "cuantiamonetariapendienteflag",
		WH_Requisiciones.Comentarios as "comentario",
		WH_Requisiciones.DireccionDestino as "direcciondestino",
		WH_Requisiciones.Responsable as "responsable",
		SY_CampoCalculado.Flag01 as "flag01",
		WH_ClasificacionRequOrden.DescripcionLocal as "descripcionlocal",
		AC_CostCenterMst.LocalName as "localname",
		WH_AlmacenMast.DescripcionLocal as "descripcionlocalalmacen",
		WH_Requisiciones.AlmacenCodigo as "almacencodigo",
		WH_Requisiciones.CotizadoFlag as "cotizadoflag",
		WH_Requisiciones.Sucursal as "sucursal",
		AC_Sucursal.DESCRIPCIONLOCAL as "descripcionsucursal", 
		WH_Requisiciones.TransaccionOperacion as "transaccionoperacion",
		WH_ItemClasificacionComercial.DescripcionLocal as "descripcionlocalitemclasi",
		PersonaMast.NombreCompleto as "personarevisiontecnica",
		WH_Requisiciones.RevisionTecnicaPor as "revisiontecnicapor"
		FROM
		SGCORESYS.WH_Requisiciones
		LEFT JOIN SGCORESYS.WH_ClasificacionRequOrden ON
		(WH_Requisiciones.Clasificacion =
		WH_ClasificacionRequOrden.Clasificacion)
		LEFT JOIN
		SGCORESYS.WH_AlmacenMast ON
		(WH_Requisiciones.AlmacenCodigo = WH_AlmacenMast.AlmacenCodigo)
		LEFT JOIN SGCORESYS.AC_CostCenterMst
		ON
		(WH_Requisiciones.DefaultPrime = AC_CostCenterMst.CostCenter)
		LEFT JOIN sgcoresys.WH_ItemClasificacionComercial 
		ON 
		(WH_Requisiciones.TransaccionOperacion = WH_ItemClasificacionComercial.ClasificacionComercial 
		AND WH_ItemClasificacionComercial.Estado = 'A')
		LEFT JOIN SGCORESYS.PERSONAMAST
		ON
		(WH_Requisiciones.RevisionTecnicaPor = PERSONAMAST.Persona)
		LEFT JOIN SGCORESYS.AC_SUCURSAL
        ON  
        (WH_Requisiciones.Sucursal = AC_Sucursal.SUCURSAL  AND AC_Sucursal.Estado = 'A'),
		SGCORESYS.SY_CampoCalculado
		WHERE
		( WH_Requisiciones.RequisicionNumero >= nvl(:p_requisicionnumero,WH_Requisiciones.RequisicionNumero) ) AND
		( SY_CampoCalculado.RegistroNumero = 1 )  
		 ]]>
	</sql-query>

