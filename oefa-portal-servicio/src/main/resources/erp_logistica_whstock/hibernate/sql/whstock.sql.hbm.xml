	<!-- FALTA MAPEAR whstock.dw_wh_po_list_securitycia -->
	<sql-query name="whstock.DwWhPoListSecurity">
		SELECT
		WH_NivelAprobacion.NivelAprobacion as "nivelaprobacion",
		WH_NivelAprobacion.MontoDesde as "montodesde",
		WH_NivelAprobacion.MontoHasta as "montohasta",
		WH_NivelAprobacion.ContratoFlag as "contratoflag",
		WH_NivelAprobacion.BoletaFlag as "boletaflag"
		FROM
		SGCORESYS.WH_NivelAprobacion
		INNER JOIN SGCORESYS.WH_PersonaNivel
		ON (WH_NivelAprobacion.TipoNivel = WH_PersonaNivel.TipoNivel AND
		WH_NivelAprobacion.TipoDocumento = WH_PersonaNivel.TipoDocumento AND
		WH_NivelAprobacion.Secuencia = WH_PersonaNivel.Secuencia)
		INNER JOIN SGCORESYS.EmpleadoMast
		ON (EmpleadoMast.Empleado = WH_PersonaNivel.Persona)
		WHERE
		( WH_PersonaNivel.TipoDocumento = :p_tipodocumento ) AND
		( EmpleadoMast.CodigoUsuario = :p_userid )
		ORDER BY 2 ASC

	</sql-query>

	<!-- FALTA MAPEAR whstock.dw_wh_po_list_securitycia -->
	<sql-query name="whstock.DwWhPoListSecuritycia">
		SELECT
		WH_NivelAprobacionCompania.NivelAprobacion as "nivelaprobacion",
		WH_NivelAprobacionCompania.MontoDesde as "montodesde",
		WH_NivelAprobacionCompania.MontoHasta as "montohasta",
		WH_NivelAprobacionCompania.CompaniaSocio as "companiasocio",
		EmpleadoMast.CodigoUsuario as "codigousuario",
		WH_NivelAprobacionCompania.MonedaCodigo as "monedacodigo"
		FROM
		SGCORESYS.WH_PersonaNivelCompania
		INNER JOIN SGCORESYS.EmpleadoMast
		ON (WH_PersonaNivelCompania.Persona = EmpleadoMast.Empleado )
		INNER JOIN SGCORESYS.WH_NivelAprobacionCompania
		ON (WH_PersonaNivelCompania.TipoNivel =
		WH_NivelAprobacionCompania.TipoNivel AND
		WH_PersonaNivelCompania.TipoDocumento =
		WH_NivelAprobacionCompania.TipoDocumento AND
		WH_PersonaNivelCompania.Secuencia =
		WH_NivelAprobacionCompania.Secuencia AND
		WH_NivelAprobacionCompania.CompaniaSocio =
		WH_PersonaNivelCompania.CompaniaSocio)
		WHERE
		( ( WH_PersonaNivelCompania.TipoDocumento = :p_tipodocumento ) AND
		( EmpleadoMast.CodigoUsuario = :p_userid ) AND
		( WH_PersonaNivelCompania.CompaniaSocio = :p_compania ) )
		ORDER BY
		2 ASC,
		3 ASC
	</sql-query>

