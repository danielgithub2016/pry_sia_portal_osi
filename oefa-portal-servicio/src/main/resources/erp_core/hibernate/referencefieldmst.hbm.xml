<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.royal.erp.core.dominio.Referencefieldmst" table="SGCORESYS.REFERENCEFIELDMST">
        <composite-id name="pk" class="net.royal.erp.core.dominio.ReferencefieldmstPk">
            <key-property name="referencefield" type="string">
                <column name="REFERENCEFIELD" length="2" />
            </key-property>
        </composite-id>
        <property name="localname" type="string">
            <column name="LOCALNAME" length="30" />
        </property>
        <property name="englishname" type="string">
            <column name="ENGLISHNAME" length="30" />
        </property>
        <property name="screentitle" type="string">
            <column name="SCREENTITLE" length="30" />
        </property>
        <property name="length" type="integer">
            <column name="LENGTH" length="22" />
        </property>
        <property name="internalcodeflag" type="string">
            <column name="INTERNALCODEFLAG" length="1" />
        </property>
        <property name="internalcode" type="string">
            <column name="INTERNALCODE" length="10" />
        </property>
        <property name="tablevalidatedflag" type="string">
            <column name="TABLEVALIDATEDFLAG" length="1" />
        </property>
        <property name="availableflag" type="string">
            <column name="AVAILABLEFLAG" length="1" />
        </property>
        <property name="status" type="string">
            <column name="STATUS" length="1" />
        </property>
        <property name="lastuser" type="string">
            <column name="LASTUSER" length="20" />
        </property>
        <property name="lastdate" type="timestamp">
            <column name="LASTDATE" length="7" />
        </property>
    </class>
</hibernate-mapping>
