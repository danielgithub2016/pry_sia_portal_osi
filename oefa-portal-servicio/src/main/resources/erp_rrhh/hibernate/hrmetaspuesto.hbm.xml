<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.royal.erp.rrhh.dominio.HrMetaspuesto" table="SGCORESYS.HR_METASPUESTO">
        <composite-id name="pk" class="net.royal.erp.rrhh.dominio.HrMetaspuestoPk">
            <key-property name="codigopuesto" type="integer">
                <column name="CODIGOPUESTO" length="22" />
            </key-property>
            <key-property name="secuencia" type="integer">
                <column name="SECUENCIA" length="22" />
            </key-property>
        </composite-id>
        <property name="descripcion" type="string">
            <column name="DESCRIPCION" length="100" />
        </property>
        <property name="estado" type="string">
            <column name="ESTADO" length="1" />
        </property>
        <property name="secuenciaeval" type="integer">
            <column name="SECUENCIAEVAL" length="22" />
        </property>
        <property name="factorparticipacion" type="integer">
            <column name="FACTORPARTICIPACION" length="22" />
        </property>
    </class>
</hibernate-mapping>
