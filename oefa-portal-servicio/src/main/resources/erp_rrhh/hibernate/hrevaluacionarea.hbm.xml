<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.royal.erp.rrhh.dominio.HrEvaluacionarea" table="SGCORESYS.HR_EVALUACIONAREA">
        <composite-id name="pk" class="net.royal.erp.rrhh.dominio.HrEvaluacionareaPk">
            <key-property name="area" type="string">
                <column name="AREA" length="3" />
            </key-property>
        </composite-id>
        <property name="descripcion" type="string">
            <column name="DESCRIPCION" length="100" />
        </property>
        <property name="ponderacion" type="integer">
            <column name="PONDERACION" length="22" />
        </property>
        <property name="estado" type="string">
            <column name="ESTADO" length="1" />
        </property>
        <property name="tipoevaluacion" type="string">
            <column name="TIPOEVALUACION" length="1" />
        </property>
        <property name="orden" type="integer">
            <column name="ORDEN" length="22" />
        </property>
    </class>
</hibernate-mapping>
