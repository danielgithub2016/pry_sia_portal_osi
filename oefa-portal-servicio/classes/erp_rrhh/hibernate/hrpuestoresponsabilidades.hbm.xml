<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.royal.erp.rrhh.dominio.HrPuestoresponsabilidades" table="SGCORESYS.HR_PUESTORESPONSABILIDADES">
        <composite-id name="pk" class="net.royal.erp.rrhh.dominio.HrPuestoresponsabilidadesPk">
            <key-property name="codigopuesto" type="integer">
                <column name="CODIGOPUESTO" length="22" />
            </key-property>
            <key-property name="codigoreponsabilidad" type="integer">
                <column name="CODIGOREPONSABILIDAD" length="22" />
            </key-property>
        </composite-id>
        <property name="comentario" type="string">
            <column name="COMENTARIO" length="4000" />
        </property>
        <property name="estado" type="string">
            <column name="ESTADO" length="1" />
        </property>
    </class>
</hibernate-mapping>
