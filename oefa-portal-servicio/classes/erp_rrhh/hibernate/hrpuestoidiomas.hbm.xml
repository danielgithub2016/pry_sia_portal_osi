        <property name="niveloral" type="string">
            <column name="NIVELORAL" length="1" />
        </property>
        <property name="nivelescritura" type="string">
            <column name="NIVELESCRITURA" length="1" />
        </property>
        <property name="nivelgeneral" type="string">
            <column name="NIVELGENERAL" length="1" />
        </property>
        <property name="estado" type="string">
            <column name="ESTADO" length="1" />
        </property>
    </class>
</hibernate-mapping>
