		<property name="cargo" type="string">
			<column name="CARGO" length="3" />
		</property>
		<property name="categoria" type="string">
			<column name="CATEGORIA" length="5" />
		</property>
		<property name="catsigla" type="string">
			<column name="CATSIGLA" length="10" />
