<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.royal.erp.rrhh.dominio.HrPuestoinformatica" table="SGCORESYS.HR_PUESTOINFORMATICA">
        <composite-id name="pk" class="net.royal.erp.rrhh.dominio.HrPuestoinformaticaPk">
            <key-property name="codigopuesto" type="integer">
                <column name="CODIGOPUESTO" length="22" />
            </key-property>
            <key-property name="informatica" type="string">
                <column name="INFORMATICA" length="10" />
            </key-property>
        </composite-id>
        <property name="nivel" type="string">
            <column name="NIVEL" length="1" />
        </property>
        <property name="gradocuantitativo" type="string">
            <column name="GRADOCUANTITATIVO" length="2" />
        </property>
        <property name="puntosgrado" type="integer">
            <column name="PUNTOSGRADO" length="22" />
        </property>
        <property name="porcentajegrado" type="integer">
            <column name="PORCENTAJEGRADO" length="22" />
        </property>
    </class>
</hibernate-mapping>
