        <property name="conceptoaccesodefault" type="string">
            <column name="CONCEPTOACCESODEFAULT" length="4" />
        </property>
        <property name="requierejustificacion" type="string">
            <column name="REQUIEREJUSTIFICACION" length="1" />
        </property>
        <property name="estado" type="string">
            <column name="ESTADO" length="1" />
        </property>
        <property name="autojustificable" type="string">
            <column name="AUTOJUSTIFICABLE" length="1" />
        </property>
    </class>
</hibernate-mapping>
