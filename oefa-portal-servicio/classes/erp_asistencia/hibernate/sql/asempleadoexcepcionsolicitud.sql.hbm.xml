<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <sql-query name="asempleadoexcepcion.eliminarEmpleadoExcepcionSolicitud">
		DELETE FROM SGCORESYS.AS_EmpleadoExcepcionSolicitud WHERE Empleado = :p_empleado 
				AND FechaSolicitud = :p_fecha
    </sql-query>
</hibernate-mapping>
