<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.royal.erp.core.dominio.MaMiscelaneosheader" table="SGCORESYS.MA_MISCELANEOSHEADER">
        <composite-id name="pk" class="net.royal.erp.core.dominio.MaMiscelaneosheaderPk">
            <key-property name="aplicacioncodigo" type="string">
                <column name="APLICACIONCODIGO" length="2" />
            </key-property>
            <key-property name="codigotabla" type="string">
                <column name="CODIGOTABLA" length="10" />
            </key-property>
            <key-property name="compania" type="string">
                <column name="COMPANIA" length="6" />
            </key-property>
        </composite-id>
        <property name="descripcionlocal" type="string">
            <column name="DESCRIPCIONLOCAL" length="40" />
        </property>
        <property name="descripcionextranjera" type="string">
            <column name="DESCRIPCIONEXTRANJERA" length="40" />
        </property>
        <property name="estado" type="string">
            <column name="ESTADO" length="1" />
        </property>
    </class>
</hibernate-mapping>
