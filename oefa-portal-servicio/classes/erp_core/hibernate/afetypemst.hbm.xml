<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.royal.erp.core.dominio.Afetypemst" table="SGCORESYS.AFETYPEMST">
        <composite-id name="pk" class="net.royal.erp.core.dominio.AfetypemstPk">
            <key-property name="afetype" type="string">
                <column name="AFETYPE" length="2" />
            </key-property>
        </composite-id>
        <property name="localname" type="string">
            <column name="LOCALNAME" length="40" />
        </property>
        <property name="englishname" type="string">
            <column name="ENGLISHNAME" length="40" />
        </property>
        <property name="status" type="string">
            <column name="STATUS" length="1" />
        </property>
        <property name="lastuser" type="string">
            <column name="LASTUSER" length="20" />
        </property>
        <property name="lastdate" type="timestamp">
            <column name="LASTDATE" length="7" />
        </property>
        <property name="account" type="string">
            <column name="ACCOUNT" length="20" />
        </property>
    </class>
</hibernate-mapping>
