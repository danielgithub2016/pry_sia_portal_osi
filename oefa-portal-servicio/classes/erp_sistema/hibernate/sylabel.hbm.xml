<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.royal.erp.sistema.dominio.SyLabel" table="SGCORESYS.SY_LABEL">
        <composite-id name="pk" class="net.royal.erp.sistema.dominio.SyLabelPk">
            <key-property name="application" type="string">
                <column name="APPLICATION" length="2" />
            </key-property>
            <key-property name="id" type="string">
                <column name="ID" length="25" />
            </key-property>
        </composite-id>
        <property name="label" type="string">
            <column name="LABEL" length="50" />
        </property>
    </class>
</hibernate-mapping>
